{
  "name": "movie-search-cli",
  "version": "0.1.0",
  "description": "Searches movies from the omdpapi and stores them to a file by ranksing",
  "dependencies": {
    "async": "*",
    "fs-extra": "*",
    "request": "*",
    "underscore": "*",
    "xls-to-json": "*",
    "ya-csv": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "chai": "*",
    "coveralls": "^2.11.2",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "0.6.4 ",
    "grunt-coveralls": "^1.0.0",
    "grunt-mocha-test": "^0.12.7",
    "mocha": "2.2.1",
    "mocha-lcov-reporter": "0.0.2",
    "q": "*"
  },
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
